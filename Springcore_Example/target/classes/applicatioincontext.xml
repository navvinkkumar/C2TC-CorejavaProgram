<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id ="student1" class="com.tnsif.Springcore_Example.Student">
	<property name="name" value="Adithyan"/>
	</bean>
	
	<bean id ="student2" class="com.tnsif.Springcore_Example.Student">
	<property name="name" value="Abineshwar"/>
	</bean>
	
	<bean id ="mvit" class="com.tnsif.Springcore_Example.College">
	<property name="student" ref="student1"/>
	</bean>
	
	<bean id ="smvec" class="com.tnsif.Springcore_Example.College">
	<property name="student" ref="student2"/>
	</bean>
</beans>